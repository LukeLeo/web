
<script type="text/javascript"><!--

$(function() {

  var createIt = function() {
    var params = {
         major: cd.chosenMajor(),
         minor: cd.chosenMinor()
    };
    $.getJSON('/setup_default_start_point/save_no_exercise', params, function(dojo) {
      cd.newDojoDialog(dojo.id, 'from_setup');
    });
  };

  $('#no-exercise').click(function() {
    $.getJSON('/image_puller/language_pull_needed', cd.chosenMajorMinor(), function(pull) {
      if (pull.needed) {
        cd.dialog_pullImageThen('/image_puller/language_pull', createIt);
      } else {
        createIt();
      }
    });
  });

});

//--></script>

<button type="button" id="no-exercise">no exercise</button>
