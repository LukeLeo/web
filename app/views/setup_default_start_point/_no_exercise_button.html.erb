
<script type="text/javascript"><!--

$(function() {

  var makeParams = function() {
    return {
         major: "<%= @language %>",
         minor: "<%= @test %>"
    };
  };

  var createIt = function() {
    $.getJSON('/setup_default_start_point/save_no_exercise', makeParams(), function(dojo) {
      cd.newDojoDialog(dojo.id, 'from_setup');
    });
  };

  $('#no-exercise').click(function() {
    var params = makeParams();
    params['type'] = 'language'
    cd.dialog_pullImageIfNeededThen(params, createIt);
  });

});

//--></script>

<button type="button" id="no-exercise">set it up with no exercise</button>
