
<script type="text/javascript"><!--

$(function() {

  $('#create-it').click(function() {
    var setupParams = {
         major: "<%= @language %>",
         minor: "<%= @test %>",
      exercise: $('[id^=exercises_name_][class~=selected]').data('name')
    };
    $.getJSON('/setup_default_start_point/save', setupParams, function(dojoParams) {
      cd.newDojoDialog(dojoParams, 'from_setup');
    });
  });

});

//--></script>

<button type="button" id="create-it">set it up</button>

