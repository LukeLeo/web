
<div id="languages-list">
  <% @start_points.major_names.each_with_index do |major_name, index| %>
    <div class="filename"
         id="major_<%= index %>"
         data-major="<%= major_name %>"
         data-index="<%= index %>"
         data-minor-index="<%= @start_points.minor_indexes[index][0] %>">
      <%= major_name %>
    </div>
  <% end %>
</div>